{
  "AutoMigrate": true,
  "ConnectionStrings": {
    "Default": "Host=localhost;Port=5432;Database=name;Username=name;Password=secret"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Workers": {
    "DefaultGuildId": "default",
    "ExpirePunishments": {
      "Enabled": true,
      "IntervalSeconds": 10,
      "BatchSize": 100
    },
    "Roulette": {
      "Enabled": true,
      "TickSeconds": 5
    }
  }
}
